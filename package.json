{
  "name": "Feather",
  "version": "1.1.81",
  "description": "My framework",
  "repository": "https://github.com/mendrik/feather",
  "main": "feather.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:css": "postcss --c conf/postcss.config.js -o out/stylesheets/feather.css src/pcss/feather.pcss",
    "watch:css": "postcss --c conf/postcss.config.js -w -o out/stylesheets/feather.css src/pcss/feather.pcss",
    "tslint": "tslint -c conf/tslint.json src/typescript/**/*.ts",
    "build:feather": "node node_modules/typescript/lib/tsc.js -d -p src/typescript/feather/tsconfig.json",
    "build:demo": "node node_modules/typescript/lib/tsc.js -d -p src/typescript/demo/tsconfig.json",
    "watch:feather": "node node_modules/typescript/lib/tsc.js -w -d -p src/typescript/feather/tsconfig.json",
    "watch:demo": "node node_modules/typescript/lib/tsc.js -d -w -p src/typescript/demo/tsconfig.json",
    "min:feather": "npm run min:lib && npm run build:feather && node conf/uglify.js",
    "build": "npm run min:feather",
    "start": "node node_modules/npm-run-all/bin/npm-run-all/index.js --parallel watch:feather watch:demo min:lib watch:css",
    "min:lib": "node conf/uglify-libs.js",
    "pretest": "npm run build:feather && npm run build:demo",
    "test": "node node_modules/mocha/bin/mocha"
  },
  "author": "Andreas Prudzilko",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "4.0.1",
    "@types/es6-shim": "0.31.34",
    "@types/jsdom": "11.0.1",
    "@types/mocha": "2.2.41",
    "@types/node": "8.0.17",
    "@types/power-assert": "1.4.29",
    "@types/sinon": "2.3.3",
    "@types/sinon-chai": "2.7.28",
    "autoprefixer": "7.1.2",
    "chai": "4.1.1",
    "chai-sinon": "2.8.1",
    "jsdom": "11.1.0",
    "mocha": "3.4.2",
    "mocha-bamboo-reporter": "1.1.1",
    "nodemon": "1.11.0",
    "npm-run-all": "^4.0.2",
    "postcss-clean": "1.0.3",
    "postcss-cli": "4.1.0",
    "postcss-custom-properties": "6.1.0",
    "postcss-discard-comments": "2.0.4",
    "postcss-import": "10.0.0",
    "postcss-math": "0.0.8",
    "postcss-mixins": "6.1.0",
    "power-assert": "1.4.4",
    "precss": "2.0.0",
    "replace": "0.3.0",
    "sinon": "2.4.1",
    "sinon-chai": "2.12.0",
    "stylelint": "8.0.0",
    "ts-node": "3.3.0",
    "tslib": "1.7.1",
    "tslint": "5.6.0",
    "typescript": "2.4.2",
    "uglify-js": "3.0.28"
  },
  "typeRoots": [
    "./node_modules/@types"
  ],
  "dependencies": {},
  "types": [
    "mocha",
    "node",
    "assert",
    "chai",
    "sinon-chai",
    "jsdom",
    "es6-shim",
    "sinon"
  ]
}
