{
  "compileOnSave": true,
  "compilerOptions": {
    "pretty": true,
    "sourceMap": true,
    "sourceRoot" : "./",
    "inlineSources": true,
    "noImplicitAny": false,
    "declaration": false,
    "noEmitOnError": false,
    "noEmitHelpers": true,
    "moduleResolution": "node",
    "removeComments": true,
    "rootDir": "./",
    "target": "es5",
    "experimentalDecorators": true,
    "out": "out/javascripts/feather.js"
  },
  "files": [
    "utils/functions.ts",
    "utils/objects.ts",
    "utils/arrays.ts",
    "utils/dom.ts",
    "utils/types.ts",
    "utils/strings.ts",
    "boot.ts",
    "annotations/timings.ts",
    "annotations/template.ts",
    "annotations/event.ts",
    "annotations/hub.ts",
    "annotations/route.ts",
    "annotations/bind.ts",
    "annotations/factory.ts",
    "annotations/xhr.ts",
    "widget.ts"
  ]
}
